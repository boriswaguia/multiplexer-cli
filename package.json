{
  "name": "multiplexer",
  "description": "Systems specifications correctness verifier. Find unspecified executions path in your system. A mix of Alloy and TLA+",
  "version": "0.0.1",
  "author": "Boris Waguia @boriswaguia",
  "bin": {
    "multiplexer": "./bin/run"
  },
  "bugs": "https://github.com/boriswaguia/multiplexer/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^2.2.3",
    "tslib": "^1.11.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^10.17.21",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "jest": "^26.0.1",
    "memfs": "^3.1.2",
    "ts-jest": "^25.5.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/boriswaguia/multiplexer",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "multiplexer",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "boriswaguia/multiplexer",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc --fix",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
